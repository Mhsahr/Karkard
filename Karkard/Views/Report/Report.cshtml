@model List<Work>
@{
    ViewData["Title"] = "Report";

}

<h1 style="color:blue">@ViewData["Title"]</h1>
<hr />




<div class="wrapper">
    <div class="container"> 
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">From</th>
            <th scope="col">Until</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.From</td>
                <td>@item.Until</td>
        </tbody>
    }
</table>
</div>
</div>



